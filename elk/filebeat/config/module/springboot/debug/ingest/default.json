{
  "description": "Pipeline for parsing springboot debug logs",
  "processors": [
    {
      "grok": {
        "field": "message",
        "patterns": [
          "%{TIMESTAMP_ISO8601:springboot.timestamp}%{SPACE}%{DATA:app_name}%{SPACE}%{LOGLEVEL:level}%{SPACE}%{NUMBER:pid}%{SPACE}---%{SPACE}\\[%{DATA:thread_name}\\]%{SPACE}%{DATA:logger_name}%{SPACE}:%{SPACE}%{GREEDYMULTILINE:message}",
          "%{GREEDYDATA:message}"
        ],
        "ignore_missing": true,
        "pattern_definitions": {
          "GREEDYMULTILINE": "(.|\n)*"
        }
      }
    },
    {
      "date": {
        "field": "springboot.timestamp",
        "target_field": "@timestamp",
        "formats": [
          "yyyy-MM-dd HH:mm:ss.SSS"
        ],
        "timezone": "Asia/Shanghai",
        "ignore_failure": true
      }
    },
    {
      "remove": {
        "field": "springboot.timestamp"
      }
    }
  ],
  "on_failure": [
    {
      "set": {
        "field": "error.message",
        "value": "{{ _ingest.on_failure_message }}"
      }
    }
  ]
}
